{
    "pages": {
        "configuration": {
            "tabs": [
                {
                    "name": "logging",
                    "title": "Logging",
                    "entity": [
                        {
                            "type": "singleSelect",
                            "label": "Log level",
                            "options": {
                                "disableSearch": true,
                                "autoCompleteFields": [
                                    {
                                        "value": "DEBUG",
                                        "label": "DEBUG"
                                    },
                                    {
                                        "value": "INFO",
                                        "label": "INFO"
                                    },
                                    {
                                        "value": "WARN",
                                        "label": "WARN"
                                    },
                                    {
                                        "value": "ERROR",
                                        "label": "ERROR"
                                    },
                                    {
                                        "value": "CRITICAL",
                                        "label": "CRITICAL"
                                    }
                                ]
                            },
                            "defaultValue": "INFO",
                            "field": "loglevel",
                            "required": true,
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Log level must be one of: DEBUG, INFO, WARN, ERROR, CRITICAL",
                                    "pattern": "^DEBUG|INFO|WARN|ERROR|CRITICAL$"
                                }
                            ]
                        }
                    ]
                }
            ],
            "title": "Configuration",
            "description": "Set up your add-on"
        },
        "inputs": {
            "services": [
                {
                    "template": "input_with_helper",
                    "name": "server_input",
                    "entity": [
                        {
                            "type": "text",
                            "label": "Name",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Input Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z\\w-]*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "name",
                            "help": "A unique name for the data input.",
                            "required": true
                        },
                        {
                            "field": "index",
                            "label": "Index",
                            "type": "singleSelect",
                            "defaultValue": "default",
                            "help": "An index to save File Notifications as XML.",
                            "options": {
                                "endpointUrl": "data/indexes",
                                "createSearchChoice": true,
                                "denyList": "^_.*$"
                            },
                            "required": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 1,
                                    "maxLength": 80,
                                    "errorMsg": "Length of index name should be between 1 and 80."
                                }
                            ]
                        },
                        {
                            "type": "text",
                            "label": "Sourcetype",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Sourcetype must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z\\w-]*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "sourcetype",
                            "help": "A unique sourcetype for the data.",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Add-on Server IP",
                            "field": "Server_IP",
                            "help": "IP address for the Add-on as an External Server. Please see the external engine settings of FPolicy. (Use the external IP of the Splunk instance.)",
                            "defaultValue": "0.0.0.0",
                            "required": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 7,
                                    "maxLength": 15,
                                    "errorMsg": "IP only."
                                }
                            ]
                        },
                        {
                            "type": "text",
                            "label": "Add-on Server Port",
                            "field": "Server_Port",
                            "help": "Port number for the Add-on as an External Server. Please see the external engine settings of FPolicy. (Use an unused port number for each input.)",
                            "defaultValue": "1337",
                            "required": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Port number only.",
                                    "minLength": 2,
                                    "maxLength": 5
                                }
                            ]
                        },
                        {
                            "type": "text",
                            "label": "Policy Name",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Input Name must begin with a letter and consist exclusively of alphanumeric characters and underscores or hyphen.",
                                    "pattern": "^[a-zA-Z\\w-]*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 30",
                                    "minLength": 1,
                                    "maxLength": 30
                                }
                            ],
                            "field": "Policy_Name",
                            "help": "FPolicy File Policy Name. Please see the 'vserver fpolicy policy show' output. (Use the same Policy Name for a successful handshake with FPolicy. )",
                            "required": true
                        },
                        {
                            "type": "checkbox",
                            "label": "Use SSL",
                            "field": "use_ssl",
                            "defaultValue": false,
                            "help": "Check if you want to use SSL for ONTAP connection.",
                            "modifyFieldsOnValue": [
                                {
                                    "fieldValue": 1,
                                    "fieldsToModify": [
                                        {
                                            "fieldId": "sa_cert",
                                            "required": true
                                        },
                                        {
                                            "fieldId": "sa_key",
                                            "required": true
                                        }
                                    ]
                                },
                                {
                                    "fieldValue": 0,
                                    "fieldsToModify": [
                                        {
                                            "fieldId": "sa_cert",
                                            "required": false
                                        },
                                        {
                                            "fieldId": "sa_key",
                                            "required": false
                                        }
                                    ]
                                },
                                {
                                    "fieldValue": "[[any_other_value]]",
                                    "fieldsToModify": [
                                        {
                                            "fieldId": "sa_cert",
                                            "required": false
                                        },
                                        {
                                            "fieldId": "sa_key",
                                            "required": false
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "type": "file",
                            "label": "SA Certificate",
                            "help": "Upload server side SSL certificate.",
                            "field": "sa_cert",
                            "options": {
                                "fileSupportMessage": ".pem or .txt file",
                                "supportedFileTypes": [
                                    "pem",
                                    "txt"
                                ],
                                "maxFileSize": 100,
                                "useBase64Encoding": false,
                                "display": true
                            },
                            "encrypted": false,
                            "required": false,
                            "defaultValue": "empty"
                        },
                        {
                            "type": "file",
                            "label": "SA Key",
                            "help": "Upload server side SSL Key.",
                            "field": "sa_key",
                            "options": {
                                "fileSupportMessage": ".pem or .txt file",
                                "supportedFileTypes": [
                                    "pem",
                                    "txt"
                                ],
                                "maxFileSize": 100,
                                "useBase64Encoding": false,
                                "display": true
                            },
                            "encrypted": false,
                            "required": false,
                            "defaultValue": "empty"
                        }
                    ],
                    "title": "server_input"
                }
            ],
            "title": "Inputs",
            "description": "Manage your data inputs",
            "table": {
                "actions": [
                    "edit",
                    "delete",
                    "clone"
                ],
                "header": [
                    {
                        "label": "Name",
                        "field": "name"
                    },
                    {
                        "label": "Index",
                        "field": "index"
                    },
                    {
                        "label": "Sourcetype",
                        "field": "sourcetype"
                    },
                    {
                        "label": "Status",
                        "field": "disabled"
                    }
                ],
                "moreInfo": [
                    {
                        "label": "Name",
                        "field": "name"
                    },
                    {
                        "label": "Index",
                        "field": "index"
                    },
                    {
                        "label": "Sourcetype",
                        "field": "sourcetype"
                    },
                    {
                        "label": "Status",
                        "field": "disabled",
                        "mapping": {
                            "true": "Disabled",
                            "false": "Enabled"
                        }
                    },
                    {
                        "label": "Add-on Server Port",
                        "field": "Server_Port"
                    },
                    {
                        "label": "Add-on Server IP",
                        "field": "Server_IP"
                    },
                    {
                        "label": "FPolicy Name",
                        "field": "Policy_Name"
                    },
                    {
                        "label": "Use SSL",
                        "field": "use_ssl",
                        "mapping": {
                            "1": "Yes",
                            "0": "No"
                        }
                    }
                ]
            }
        },
        "dashboard": {
            "panels": [
                {
                    "name": "default"
                }
            ]
        }
    },
    "meta": {
        "name": "fpolicy_addon_for_splunk",
        "restRoot": "fpolicy_addon_for_splunk",
        "version": "1.8.1",
        "displayName": "FPolicy Add-on for Splunk",
        "schemaVersion": "0.0.9",
        "defaultView": "search",
        "_uccVersion": "5.59.0"
    }
}
